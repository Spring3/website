{
  "name": "website",
  "version": "0.0.1",
  "description": "My personal website",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby clean && gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "lint": "eslint . --fix --ext js,jsx,ts,tsx",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "dependencies": {
    "gatsby": "^2.32.11",
    "gatsby-plugin-catch-links": "^2.3.11",
    "gatsby-plugin-gdpr-cookies": "^1.0.11",
    "gatsby-plugin-manifest": "^2.4.18",
    "gatsby-plugin-nprogress": "^2.3.10",
    "gatsby-plugin-offline": "^3.2.17",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^2.7.1",
    "gatsby-plugin-styled-components": "^4.0.0",
    "gatsby-remark-copy-linked-files": "^2.3.11",
    "gatsby-remark-images": "^3.3.18",
    "gatsby-source-filesystem": "^2.3.19",
    "gatsby-transformer-remark": "^2.8.25",
    "gatsby-transformer-sharp": "^2.5.11",
    "lodash.clamp": "^4.0.3",
    "lodash.throttle": "^4.1.1",
    "mdi-react": "^7.4.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-responsive-carousel": "^3.2.18",
    "react-spring": "^9.0.0-rc.3",
    "react-use": "^15.3.4",
    "react-use-gesture": "^8.0.1",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "1.12.0",
    "eslint": "7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.2.5",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Spring3/website.git"
  },
  "author": "Daniyil Vasylenko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Spring3/website/issues"
  },
  "homepage": "https://github.com/Spring3/website#readme",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write .",
      "eslint --fix"
    ],
    "*.md": [
      "prettier --write ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
