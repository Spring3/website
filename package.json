    "gatsby-plugin-manifest": "^2.4.18",
    "gatsby-plugin-netlify": "^2.3.11",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-nprogress": "^2.3.10",
    "gatsby-plugin-offline": "^3.2.17",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.7.1",
    "gatsby-plugin-styled-components": "^3.3.10",
    "gatsby-remark-copy-linked-files": "^2.3.11",
    "gatsby-remark-images": "^3.3.18",
    "gatsby-source-filesystem": "^2.3.19",
    "gatsby-transformer-remark": "^2.8.25",
    "gatsby-transformer-sharp": "^2.5.11",
    "lodash.clamp": "^4.0.3",
    "lodash.throttle": "^4.1.1",
    "mdi-react": "^7.3.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-responsive-carousel": "^3.2.9",
    "react-spring": "^8.0.27",
    "react-use-gesture": "^8.0.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.5.3",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Spring3/website.git"
  },
  "author": "Daniyil Vasylenko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Spring3/website/issues"
  },
  "homepage": "https://github.com/Spring3/website#readme",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
